# vim: set ft=sh:

# Git
fresh-options --file=~/.gitconfig --marker --ignore-missing
  fresh git/alias
  fresh git/credential
fresh-options

# Gitignore
fresh-options --file=~/.gitignore --marker=#
  fresh github/gitignore Global/OSX.gitignore
  fresh github/gitignore Global/Vim.gitignore
fresh-options
